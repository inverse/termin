{
    "name": "inverse/termin",
    "description": "Simple PHP tool for finding a termin in Berlin",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "fabpot/goutte": "^3.2",
        "ivkos/pushbullet": "^3.3",
        "monolog/monolog": "^1.23",
        "pimple/pimple": "^3.2",
        "telegram-bot/api": "^2.3",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Inverse\\Termin\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Inverse\\Termin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
