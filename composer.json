{
    "name": "inverse/termin",
    "description": "Simple PHP tool for finding a termin in Berlin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-dom": "*",
        "fabpot/goutte": "^4.0.0",
        "ivkos/pushbullet": "^3.3",
        "monolog/monolog": "^2 | ^3",
        "pimple/pimple": "^3.2",
        "psr/log": "^3",
        "symfony/yaml": "^6",
        "telegram-bot/api": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Inverse\\Termin\\": "src/"
        }
    },
    "require-dev": {
        "colinodell/psr-testlogger": "^1.1",
        "phpunit/phpunit": "^9.3.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Inverse\\Termin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
