{
    "name": "inverse/termin",
    "description": "Simple PHP tool for finding a termin in Berlin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-dom": "*",
        "campo/random-user-agent": "^1.3",
        "ivkos/pushbullet": "^3.3",
        "monolog/monolog": "^2.1.1",
        "pimple/pimple": "^3.2",
        "psr/log": "^1.1.4",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/dom-crawler": "^6.3",
        "symfony/http-client": "^6.3",
        "symfony/yaml": "^6.3",
        "telegram-bot/api": "v2.3.26",
        "verifiedjoseph/ntfy-php-library": "^4"
    },
    "autoload": {
        "psr-4": {
            "Inverse\\Termin\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Inverse\\Termin\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
